{
  "agents": {
    "defaults": {
      "models": {
        "__DEFAULT_MODEL__": {}
      },
      "compaction": {
        "mode": "default",
        "memoryFlush": {
          "enabled": true
        }
      }
    },
    "list": [
      {
        "id": "main",
        "default": true
      }
    ]
